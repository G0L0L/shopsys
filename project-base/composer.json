{
    "name": "shopsys/project-base",
    "type": "project",
    "description": "Full-featured basis for your e-commerce project on Shopsys Framework",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-redis": "*",
        "ext-xml": "*",
        "arvenil/ninja-mutex": "^0.4.1",
        "bmatzner/jquery-bundle": "^2.2.2",
        "bmatzner/jquery-ui-bundle": "^1.10.3",
        "commerceguys/intl": "^1.0.0",
        "composer/composer": "^1.6.0",
        "craue/formflow-bundle": "^3.0.3",
        "doctrine/annotations": "^1.6.0",
        "doctrine/common": "^2.8.1",
        "doctrine/doctrine-bundle": "^1.8.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0.2",
        "doctrine/doctrine-migrations-bundle": "^1.3.0",
        "fp/jsformvalidator-bundle": "^1.5.1",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "fzaninotto/faker": "^1.7.1",
        "helios-ag/fm-elfinder-bundle": "^9.2",
        "heureka/overeno-zakazniky": "^2.0.6",
        "incenteev/composer-parameter-handler": "^2.1.3",
        "intervention/image": "^2.3.14",
        "jms/translation-bundle": "^1.4.1",
        "joschi127/doctrine-entity-override-bundle": "^0.7.2",
        "league/flysystem": "^1.0",
        "phing/phing": "^2.16.1",
        "presta/sitemap-bundle": "^1.5.3",
        "prezent/doctrine-translatable-bundle": "^1.0.3",
        "sensio/framework-extra-bundle": "^3.0.29",
        "sensio/generator-bundle": "^3.1.7",
        "shopsys/doctrine-orm": "^2.6.2",
        "shopsys/google-cloud-bundle": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/postgres-search-bundle": "^0.1",
        "shopsys/migrations": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/form-types-bundle": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/framework": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/frontend-api": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/plugin-interface": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/product-feed-heureka": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/product-feed-heureka-delivery": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/product-feed-zbozi": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/product-feed-google": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/read-model": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "snc/redis-bundle": "^2.1.8",
        "stof/doctrine-extensions-bundle": "^1.3.0",
        "symfony/assetic-bundle": "^2.8.2",
        "symfony/flex": "^1.4",
        "symfony/form": "^3.4",
        "symfony/monolog-bundle": "^3.3.1",
        "symfony/security-bundle": "^3.4",
        "symfony/swiftmailer-bundle": "^3.2.2",
        "symfony/translation": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony-cmf/routing": "^2.0.3",
        "symfony-cmf/routing-bundle": "^2.0.3",
        "tracy/tracy": "^2.4.13",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.4.8",
        "vasek-purchart/console-errors-bundle": "^1.0.1",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "ext-pgsql": "*",
        "ext-zip": "*",
        "codeception/codeception": "^2.4.0",
        "phpstan/phpstan": "^0.11",
        "phpunit/phpunit": "^7.0",
        "shopsys/coding-standards": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "shopsys/http-smoke-testing": "dev-mg-weird-flex-but-ok as 9.0.x-dev",
        "phpstan/phpstan-doctrine": "^0.11.2",
        "phpstan/phpstan-phpunit": "^0.11.2",
        "sspooky13/yaml-standards": "^4.2.5"
    },
    "conflict": {
        "symfony/dependency-injection": "3.4.15|3.4.16",
        "twig/twig": "2.6.1",
        "symfony/symfony": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "component-dir": "web/components",
        "platform": {
            "php": "7.2"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "../framework/packages/backend-api",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/coding-standards",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/form-types-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/framework",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/frontend-api",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/google-cloud-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/http-smoke-testing",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/migrations",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/monorepo-tools",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/plugin-interface",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/product-feed-google",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/product-feed-heureka",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/product-feed-heureka-delivery",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/product-feed-zbozi",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "../framework/packages/read-model",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Shopsys\\Environment::checkEnvironment",
            "Shopsys\\FrameworkBundle\\Command\\ComposerScriptHandler::postInstall"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Shopsys\\Environment::checkEnvironment",
            "Shopsys\\FrameworkBundle\\Command\\ComposerScriptHandler::postUpdate"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^3.4"
        },
        "incenteev-parameters": [
            {
                "file": "app/config/parameters.yml",
                "keep-outdated": true
            },
            {
                "file": "app/config/parameters_test.yml",
                "keep-outdated": true
            }
        ]
    }
}
